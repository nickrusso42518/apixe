<?xml version='1.0' encoding='UTF-8'?>
<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0" xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0">
  <native xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
    <vrf>
      <definition>
        <name>VPN1</name>
        <description>FIRST VRF</description>
        <rd>1:1</rd>
        <address-family>
          <ipv4/>
          <ipv6/>
        </address-family>
        <route-target>
          <export>
            <asn-ip>100:2</asn-ip>
          </export>
          <import>
            <asn-ip>100:1</asn-ip>
          </import>
        </route-target>
      </definition>
      <definition>
        <name>VPN2</name>
        <description>SECOND VRF</description>
        <rd>2:2</rd>
        <address-family>
          <ipv4/>
          <ipv6/>
        </address-family>
        <route-target>
          <import>
            <asn-ip>200:1</asn-ip>
          </import>
          <import>
            <asn-ip>200:2</asn-ip>
          </import>
        </route-target>
      </definition>
    </vrf>
  </native>
</data>
